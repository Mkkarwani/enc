{
  "short_name": "ENC Viewer",
  "name": "Offline ENC Viewer",
  "start_url": "./enc/index.html",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#007bff",
  "icons": [
    {
      "src": "./enc/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "./enc/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "file_handlers": [
    {
      "action": "./enc/index.html",
      "accept": {
        "application/vnd.custom.enc": [".enc"]
      }
    }
  ]
}
